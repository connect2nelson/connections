<% if !@contact.nil?%>
    $('.alert').remove();
    $('.add-github-username').hide();
    $('.add-username-form').hide();
    $("<%= j render 'consultants/github_contact', contact: @contact %>").appendTo('.contact-entry.github');
<% end %>

<% if flash[:alert]%>
    $(".alert").append("<%= flash[:alert] %>")
<% end %>
